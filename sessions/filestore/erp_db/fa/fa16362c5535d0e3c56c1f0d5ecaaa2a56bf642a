            `;rectElement.setAttribute('data-selection-client-id',clientId);return rectElement;});const caretElement=this.document.createElement('div');caretElement.style=`border-left: 2px solid ${color}; position: absolute;`;caretElement.setAttribute('data-selection-client-id',clientId);caretElement.className='oe-collaboration-caret';const caretTopSquare=this.document.createElement('div');caretTopSquare.className='oe-collaboration-caret-top-square';caretTopSquare.style['background-color']=color;caretTopSquare.setAttribute('data-client-name',clientName);caretElement.append(caretTopSquare);const selectionInfo=this._collabSelectionInfos.get(clientId);let caretAvatar=selectionInfo&&selectionInfo.avatarElement;if(!caretAvatar){caretAvatar=this.document.createElement('div');caretAvatar.className='oe-collaboration-caret-avatar';caretAvatar.style.display='none';const image=this.document.createElement('img');caretAvatar.append(image);image.onload=()=>caretAvatar.style.removeProperty('display');image.setAttribute('src',clientAvatarUrl);image.classList.add('o_object_fit_cover');caretAvatar.setAttribute('data-selection-client-id',clientId);this._collabSelectionsContainer.append(caretAvatar);}
